[*.cs]

# CS8600: Null リテラルまたは Null の可能性がある値を Null 非許容型に変換しています。
dotnet_diagnostic.CS8600.severity = silent

# CS8602: null 参照の可能性があるものの逆参照です。
dotnet_diagnostic.CS8602.severity = silent

# CS8603: Null 参照戻り値である可能性があります。
dotnet_diagnostic.CS8603.severity = silent

# CS8604: Null 参照引数の可能性があります。
dotnet_diagnostic.CS8604.severity = silent

# CS8622: パラメーターの型における参照型の NULL 値の許容が、ターゲット デリゲートと一致しません。おそらく、NULL 値の許容の属性が原因です。
dotnet_diagnostic.CS8622.severity = silent
